[chart1]

title = Your title
sizev = 1000
sizeh = 600
showlegend = true
legendcols = 4
legendunit = "&deg;C"
yaxistitle = "Celsius"
drawtofile = false

items[item1] = "line:color:title:&deg;C"
items[item2] = "state:color:title"

;# Rememeber to remove ".empty" after the file name when you're ready.
